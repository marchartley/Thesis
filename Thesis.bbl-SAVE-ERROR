      \strng{namehash}{acce1503e203e0d80df5fcf4087bc8a0}
      \strng{fullhash}{acce1503e203e0d80df5fcf4087bc8a0}
      \strng{bibnamehash}{acce1503e203e0d80df5fcf4087bc8a0}
      \strng{authorbibnamehash}{acce1503e203e0d80df5fcf4087bc8a0}
      \strng{authornamehash}{acce1503e203e0d80df5fcf4087bc8a0}
