    \entry{Bosscher1992}{article}{}
      \name{author}{2}{}{%
        {{un=0,uniquepart=base,hash=c390fb2fd45d86e777b0999696f66a6f}{%
           family={BOSSCHER},
           familyi={B\bibinitperiod},
           given={HEMMO},
