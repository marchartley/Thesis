      \field{issue}{2}
      \field{journaltitle}{Computer Graphics Forum}
      \field{title}{Sparse representation of terrains for procedural modeling}
      \field{volume}{35}
      \field{year}{2016}
      \field{pages}{177\bibrangedash 187}
      \range{pages}{11}
      \verb{doi}
      \verb 10.1111/cgf.12821
      \endverb
    \endentry
