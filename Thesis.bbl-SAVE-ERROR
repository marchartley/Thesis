      \strng{namehash}{6ca44f60bfe8b4fbe873c31a73df0b06}
      \strng{fullhash}{59e18c404e241430d690d7aa54ec8a21}
      \strng{bibnamehash}{59e18c404e241430d690d7aa54ec8a21}
      \strng{authorbibnamehash}{59e18c404e241430d690d7aa54ec8a21}
