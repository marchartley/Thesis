      \strng{namehash}{100a47dfa732d0facc3bb0270e3fdc96}
      \strng{fullhash}{100a47dfa732d0facc3bb0270e3fdc96}
      \strng{bibnamehash}{100a47dfa732d0facc3bb0270e3fdc96}
      \strng{authorbibnamehash}{100a47dfa732d0facc3bb0270e3fdc96}
