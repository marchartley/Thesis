      \strng{namehash}{a4cfd2dd77c70123d5c36832fb64ddc7}
      \strng{fullhash}{a4cfd2dd77c70123d5c36832fb64ddc7}
      \strng{bibnamehash}{a4cfd2dd77c70123d5c36832fb64ddc7}
      \strng{authorbibnamehash}{a4cfd2dd77c70123d5c36832fb64ddc7}
