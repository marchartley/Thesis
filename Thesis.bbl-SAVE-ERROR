      \strng{namehash}{2feafceb59e6a1275258d53a8a082652}
      \strng{fullhash}{2feafceb59e6a1275258d53a8a082652}
      \strng{bibnamehash}{2feafceb59e6a1275258d53a8a082652}
      \strng{authorbibnamehash}{2feafceb59e6a1275258d53a8a082652}
