      \field{issue}{6}
      \field{journaltitle}{Computer Graphics Forum}
      \field{title}{A survey on procedural modelling for virtual worlds}
      \field{volume}{33}
      \field{year}{2014}
      \field{pages}{31\bibrangedash 50}
      \range{pages}{20}
