      \strng{namehash}{64b2d223526cfe99bc42eb0ddff8ca7a}
      \strng{fullhash}{b070b4fd8672c961478219339b85a7d8}
      \strng{bibnamehash}{b070b4fd8672c961478219339b85a7d8}
