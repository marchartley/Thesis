      \strng{namehash}{6679717e53f291d86ff2ccb7715c4eef}
      \strng{fullhash}{3616b16a83e57a4e5eb776424cdc602c}
      \strng{bibnamehash}{3616b16a83e57a4e5eb776424cdc602c}
      \strng{authorbibnamehash}{3616b16a83e57a4e5eb776424cdc602c}
