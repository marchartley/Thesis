      \strng{namehash}{d8c1b244ce67ac7907517928b90f1ed8}
      \strng{fullhash}{d97b85a43ce624e5beabf58923a72a82}
      \strng{bibnamehash}{d97b85a43ce624e5beabf58923a72a82}
      \strng{authorbibnamehash}{d97b85a43ce624e5beabf58923a72a82}
