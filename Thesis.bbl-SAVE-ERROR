      \strng{namehash}{a0f2a2a8e22f0cbf6b80c2a35d58479b}
      \strng{fullhash}{87f81092bad5b2c532c7661f74d75752}
      \strng{bibnamehash}{87f81092bad5b2c532c7661f74d75752}
      \strng{authorbibnamehash}{87f81092bad5b2c532c7661f74d75752}
