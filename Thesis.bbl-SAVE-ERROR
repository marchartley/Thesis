      \field{year}{1989}
      \field{pages}{197\bibrangedash 210}
      \range{pages}{14}
      \verb{doi}
      \verb 10.1007/BF00874487
      \endverb
      \keyw{Fractal,bathymetry,fracture zone,interpolation,ridge,tectonics}
